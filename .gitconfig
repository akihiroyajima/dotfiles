[user]
	name = akihiroyajima
	email = akihiroyazima@gmail.com
[core]
	excludesfile = /Users/ietty/.gitignore_global
	editor = vim
[merge]
	tool = p4merge
[mergetool]
	keepBackUp = false
[mergetool "p4merge"]
	cmd = p4merge "$BASE" "$LOCAL" "$REMOTE" "$MERGED"
	path = p4merge
	keepTemporaries = false
	trustExitCode = false
[alias]
	co = checkout
[url "git@github.com:"]
	pushinsteadof = https://github.com/
[push]
	default = simple
