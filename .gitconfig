[user]
	name = akihiroyajima
	email = akihiroyazima@gmail.com
[core]
	excludesfile = /Users/ietty/.gitignore_global
	editor = vim
[merge]
	tool = p4merge
[mergetool]
	keepBackUp = false
[mergetool "p4merge"]
	cmd = p4merge "$BASE" "$LOCAL" "$REMOTE" "$MERGED"
	path = p4merge
	keepTemporaries = false
	trustExitCode = false
[diff]
	tool = p4merge
[difftool "p4merge"]
	cmd = /Applications/P4V/p4merge.app/Contents/Resources/launchp4merge "\"$REMOTE\"" "\"$LOCAL\""
[alias]
	co = checkout
[url "git@github.com:"]
	pushinsteadof = https://github.com/
[push]
	default = simple
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
