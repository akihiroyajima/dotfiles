[user]
	name = akihiroyajima
	email = akihiroyazima@gmail.com
[core]
	excludesfile = /Users/ietty/.gitignore_global
	editor = vim
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
[mergetool "p4merge"]
	path = p4merge
	keepTemporaries = false
	trustExitCode = false
[alias]
	co = checkout
[merge]
	tool = vimdiff
[url "git@github.com:"]
	pushinsteadof = https://github.com/
[push]
	default = simple
