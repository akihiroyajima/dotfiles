[user]
	name = Akihiro Yajima
	email = akihiroyazima@gmail.com
[core]
	excludesfile = /Users/akihiroyajima/.gitignore_global
  editor = vim
[merge]
  tool = vimdiff
[mergetool]
  keepBackUp = false
[alias]
  ad = add
  br = branch
  ci = commit
  co = checkout
  ft = fetch
  pl = pull
  ps = push
  sh = show
  st = stash
[url "git@github.com:"]
  pushinsteadof = https://github.com/
[push]
  default = simple
[filter "lfs"]
  clean = git-lfs clean %f
  smudge = git-lfs smudge %f
  required = true
